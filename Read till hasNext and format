import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int lineNumber = 0;
        
        while (sc.hasNext()) {
            String inputText = sc.nextLine();
            lineNumber++;
            System.out.printf("%d %s%n", lineNumber, inputText);
        }
    }
}
